
  <div class="nested-fields">
    <!--<label for="picture">Picture:</label>-->
    <%= f.file_field :picture, :class => 'form_group'%>

    <div class="column col-xs-12 col-sm-4">
      <label for="org_type">Organization Type:</label>
      <%= f.text_field :org_type, :class => 'form_group'%>
    </div>

    <div class="column col-xs-12 col-sm-4">
      <label for="org_sector">Organization Sector:</label>
      <%= f.text_field :org_sector, :class => 'form_group'%>
    </div>

      <p><%= link_to_add_association'Add Address', f, :addresses %></p>
      <%= f.fields_for :addresses do |builder| %>
          <%= render 'address_fields', :f => builder %>
      <% end %>

      <p><%= link_to_remove_association 'Remove' , f  %></p>
  </div>