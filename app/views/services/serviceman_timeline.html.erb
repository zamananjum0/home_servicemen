<h1>Booking Request</h1>
<br><br><br>

<div class="container pb50">
</div>


<!-- Table Style 4 -->

<div class="container pb50 ">
  <div class="row">
    <div class="col-md-12">
      <h4 class="text-center mb30 danger">Users</h4>
      <table class="table">
        <thead>
        <tr class="info">
          <th>Name</th>
          <th>Email</th>
          <th>Role</th>
          <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        <tr class="success">
          <td>Request</td>
          <% @@services.each do |req| %>
            <tr class="warning">
              <%= req.user %>
            </tr>
        <% end %>
        </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>